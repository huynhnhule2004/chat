{
  "name": "e2ee-chat-backend",
  "version": "1.0.0",
  "description": "End-to-End Encrypted Chat Backend with Node.js, Express, Socket.io, and MongoDB",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --inspect=0.0.0.0:9229 server.js",
    "dev:local": "nodemon server.js",
    "seed": "node scripts/seed.js",
    "test:api": "node test-api.js",
    "docker:build": "docker build -t chat-backend .",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build"
  },
  "keywords": [
    "chat",
    "e2ee",
    "socket.io",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.6.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "supertest": "^7.2.2"
  }
}
